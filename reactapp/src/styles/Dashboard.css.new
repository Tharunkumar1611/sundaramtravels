/* Add the media queries for bookings and expenses containers */
@media (max-width: 1200px) {
  .bookings-container,
  .expenses-container {
    grid-template-columns: repeat(2, 1fr);
  }
}

@media (max-width: 768px) {
  .bookings-container,
  .expenses-container {
    grid-template-columns: 1fr;
  }
}